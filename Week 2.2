import java.util.*;
class prog{
    public static void main(String args[]){
        Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        int b = scan.nextInt();
        if(a<2||b<2){
            System.out.println("No");
        }
        else if(a>8||b>8){
            System.out.println("Yes");
        }
        else{
            System.out.println("Maybe");
        }
    }
}
